<?xml version="1.0" encoding="utf-8"?>

<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1024 600" >

  
<ycontrol>
   	<!-- Zeile 1 -->	
		<property name="Zeile1" datatype="String" defaultvalue="xxx">
			<target type="Content" element="zeile1" />
		</property>
		
   	<!-- Zeile 2 -->
		<property name="Zeile2" datatype="String" defaultvalue="xxx">
			<target type="Content" element="zeile2" />
		</property>


    <!-- Systemtrennung -->
        <property name="Systemtrennung anzeigen" datatype="INT16" defaultvalue="0">
            <target type="Attribute" element="Systemtrennungaktiv" selector="opacity">
                <condition eq="1" output="1" />
                <condition eq="0" output="0" />
            </target>	
        </property>

    <!-- Funktion -->
        <property name="Funktion" datatype="INT16" defaultvalue="0">
        	<target type="Attribute" element="ventilvlaktiv" selector="opacity">
        	  <condition eq="0" output="1" />
        	  <condition eq="1" output="1" />
        	  <condition eq="1" output="1" />
       			<condition gt="1" output="0" />
       		</target>
        	<target type="Content" element="leitungo">
       			<condition eq="0" output="Warmwasser" />
        	  <condition eq="1" output="Warmwasser" />
       			<condition eq="2" output="Heizen" />
        	  <condition eq="3" output="" />
       			<condition eq="4" output="" />        	    
       		</target>  
        	<target type="Content" element="leitungu">
       			<condition eq="0" output="Heizen" />
        	  <condition eq="1" output="Heizen / Kühlen" />
       			<condition eq="2" output="Heizen" />
        	  <condition eq="3" output="Heizen / Kühlen" />
       			<condition eq="4" output="Warmwasser" />         	   
       		</target>
		    </property>

<!-- Ventil VL-->
        <property name="Status Ventil" datatype="Int8" defaultvalue="99">
        	<target type="Style" element="ventilfill" selector="fill">
       			<condition eq="99" output="#f542ef" />
       			<condition eq="0" output="black" />
       			<condition eq="1" output="green" />
       			<condition eq="2" output="red" />
       			<condition eq="10" output="black" />
       			<condition eq="11" output="green" />
       			<condition eq="12" output="red" />
       			<condition eq="20" output="black" />
       			<condition eq="21" output="green" />
       			<condition eq="22" output="red" />
       		</target>
       		 <target type="Style" element="ventilfill" selector="stroke">
       			<condition eq="99" output="#18c6c9" />
       			<condition lt="10" output="white" />
       			<condition lt="20" output="blue" />
       			<condition lt="30" output="orange" />
       		</target>
        	<target type="Content" element="ventiltext">
       			<condition eq="99" output="Unbekannt" />
        	    <condition eq="0" output="Auto Aus" />
       			<condition eq="1" output="Auto Ein" />
       			<condition eq="2" output="Störung" />
       			<condition eq="10" output="Hand Aus" />
       			<condition eq="11" output="Hand Ein" />
       			<condition eq="12" output="Störung" />
       			<condition eq="20" output="Lokal Aus" />
       			<condition eq="21" output="Lokal Ein" />
       			<condition eq="22" output="Störung" />
       		</target>
        </property>


    <!-- Ventil RL -->
        <property name="Ventil RL anzeigen" datatype="INT16" defaultvalue="0">
            <target type="Attribute" element="ventilrlaktiv" selector="opacity">
                <condition eq="1" output="1" />
                <condition eq="0" output="0" />
            </target>	
        </property>


    <!-- Venti -->
        <property name="Status Venti" datatype="Int8" defaultvalue="99">
        	<target type="Style" element="venticolorfill" selector="fill">
       			<condition eq="99" output="#f542ef" />
       			<condition eq="0" output="black" />
       			<condition eq="1" output="green" />
       			<condition eq="2" output="red" />
       			<condition eq="10" output="black" />
       			<condition eq="11" output="green" />
       			<condition eq="12" output="red" />
       			<condition eq="20" output="black" />
       			<condition eq="21" output="green" />
       			<condition eq="22" output="red" />
       		</target>
       		 <target type="Style" element="venticolorfill" selector="stroke">
       			<condition eq="99" output="#18c6c9" />
       			<condition lt="10" output="white" />
       			<condition lt="20" output="blue" />
       			<condition lt="30" output="orange" />
       		</target>
       		 <target type="Style" element="ventifancolorfill" selector="fill">
       			<condition eq="99" output="#18c6c9" />
       			<condition lt="10" output="white" />
       			<condition lt="20" output="blue" />
       			<condition lt="30" output="orange" />
       		</target>
		    </property>
  
<!-- Komp -->
        <property name="Status Kompressor" datatype="Int8" defaultvalue="99">
        	<target type="Style" element="kompcolorfill" selector="fill">
       			<condition eq="99" output="#f542ef" />
       			<condition eq="0" output="black" />
       			<condition eq="1" output="green" />
       			<condition eq="2" output="red" />
       			<condition eq="10" output="black" />
       			<condition eq="11" output="green" />
       			<condition eq="12" output="red" />
       			<condition eq="20" output="black" />
       			<condition eq="21" output="green" />
       			<condition eq="22" output="red" />
       		</target>
       		 <target type="Style" element="kompcolorfill" selector="stroke">
       			<condition eq="99" output="#18c6c9" />
       			<condition lt="10" output="white" />
       			<condition lt="20" output="blue" />
       			<condition lt="30" output="orange" />
       		</target>
		</property>

<!-- Status Pumpe -->
        <property name="Status Pumpe" datatype="Int8" defaultvalue="1">
        	<target type="Style" element="pumpcolorfill" selector="fill">
       			<condition eq="99" output="#f542ef" />
       			<condition eq="0" output="black" />
       			<condition eq="1" output="green" />
       			<condition eq="2" output="red" />
       			<condition eq="10" output="black" />
       			<condition eq="11" output="green" />
       			<condition eq="12" output="red" />
       			<condition eq="20" output="black" />
       			<condition eq="21" output="green" />
       			<condition eq="22" output="red" />
       		</target>
       		 <target type="Style" element="pumpcolorfill" selector="stroke">
       			<condition eq="99" output="#18c6c9" />
       			<condition lt="10" output="white" />
       			<condition lt="20" output="blue" />
       			<condition lt="30" output="orange" />
       		</target>
       		 <target type="Style" element="pumpcolorstroke" selector="fill">
       			<condition eq="99" output="#18c6c9" />
       			<condition lt="10" output="white" />
       			<condition lt="20" output="blue" />
       			<condition lt="30" output="orange" />
       		</target>
		</property>

<!-- Text WP-->
		<property name="Sollwert" datatype="String" defaultvalue="xxx">
			<target type="Content" element="sollwert" />
		</property>
		<property name="Eintritt" datatype="String" defaultvalue="xxx">
			<target type="Content" element="eintritt" />
		</property>
		<property name="Austritt" datatype="String" defaultvalue="xxx">
			<target type="Content" element="austritt" />
		</property>
		<property name="Leistung" datatype="String" defaultvalue="xxx">
			<target type="Content" element="Leistung" />
		</property>
    <property name="Stauts EZ" datatype="INT16" defaultvalue="0">
        <target type="Content" element="statusez">
       			<condition eq="0" output="Auto Aus" />
        	  <condition eq="1" output="Ein Heizen" />
       			<condition eq="2" output="Ein BWW" />
        	  <condition eq="3" output="Ein Kühlen" />
       			<condition eq="4" output="Störung!" /> 
       			<condition eq="5" output="Qittieren" />
       			<condition eq="6" output="Frostschutz" />
       			<condition eq="7" output="EVU-Sperre" />
       			<condition eq="8" output="Freecoling und Heizen" />
       			<condition eq="10" output="Hand Aus" />
       			<condition eq="90" output="Abtauung" />
       			<condition eq="100" output="EVU-Sperre" />
       		</target>  
		</property>   
    
    <!-- Status Pumpe Systemtrennung -->
    <property name="Status Pumpe Systemtrennung" datatype="Int8" defaultvalue="99">
        	<target type="Style" element="pumpsyscolorfill" selector="fill">
       			<condition eq="99" output="#f542ef" />
       			<condition eq="0" output="black" />
       			<condition eq="1" output="green" />
       			<condition eq="2" output="red" />
       			<condition eq="10" output="black" />
       			<condition eq="11" output="green" />
       			<condition eq="12" output="red" />
       			<condition eq="20" output="black" />
       			<condition eq="21" output="green" />
       			<condition eq="22" output="red" />
       		</target>
       		 <target type="Style" element="pumpsyscolorfill" selector="stroke">
       			<condition eq="99" output="#18c6c9" />
       			<condition lt="10" output="white" />
       			<condition lt="20" output="blue" />
       			<condition lt="30" output="orange" />
       		</target>
       		 <target type="Style" element="pumpsyscolorstroke" selector="fill">
       			<condition eq="99" output="#18c6c9" />
       			<condition lt="10" output="white" />
       			<condition lt="20" output="blue" />
       			<condition lt="30" output="orange" />
       		</target>
		</property>

        <!-- Status Ventil Systemtrennung -->
		<property name="Position Systemtrennung Ventil" datatype="Int16" defaultvalue="1000">
		    <target type="Content" element="ventilsyspos" />
		</property>
		
		<property name="Status Systemtrennung Ventil" datatype="Int16" defaultvalue="0">
			<target type="Style" element="ventilsyscollorfill" selector="fill">
       			<condition gt="100" output="red" />
       			<condition eq="1" output="green" />
       			<condition eq="0" output="black" />
       			<condition eq="11" output="green" />
       			<condition eq="10" output="black" />
       			<condition lt="0" output="red" />
       			</target>
			<target type="Style" element="ventilsyscollorfill" selector="stroke">
       			<condition eq="0" output="white" />
       			<condition eq="1" output="white" />
       			<condition eq="10" output="blue" />
       			<condition eq="11" output="blue" />
       			</target>
		</property>


    <!-- Fuehler RL Systemtrennung-->

      <property name="Fühler RL Systemtrennung einblenden" datatype="boolean" defaultvalue="true">
        	<target type="Attribute" element="fuehlersysrlaktiv" selector="opacity">
        		<condition eq="true" output="1" />
        		<condition eq="false" output="0" />
       		</target>
		</property>
			
		<property name="Fühler RL Systemtrennung Istwert" datatype="Int16" defaultvalue="1">
		    <target type="Content" element="fuehlersysrlist" />
			<target type="Style" element="fuehlersysrlcolorfill" selector="fill">
       			<condition gt="100" output="red" />
       			<condition gte="1" output="green" />
       			<condition lte="0" output="red" />
       			</target>
		</property>


    <!-- Fuehler VL Systemtrennung-->

    <property name="Fühler VL Systemtrennung einblenden" datatype="boolean" defaultvalue="true">
        	<target type="Attribute" element="fuehlersysvlaktiv" selector="opacity">
        		<condition eq="true" output="1" />
        		<condition eq="false" output="0" />
       		</target>
		</property>

		<property name="Fühler VL Systemtrennung Istwert" datatype="Int16" defaultvalue="1">
		    <target type="Content" element="fuehlersysvlist" />
			<target type="Style" element="fuehlersysvlcolorfill" selector="fill">
       			<condition gt="100" output="red" />
       			<condition gte="1" output="green" />
       			<condition lte="0" output="red" />
       			</target>
		</property>

	<action name="Click WP" triggers="click" element="wpclick" />
	<action name="cd" triggers="click" element="ventilclick" />
    
 



  </ycontrol>

